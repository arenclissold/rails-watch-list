<div class="container py-5">
  <div class="row py-5 justify-content-center">
    <div class="col-6">
      <h1>Add New List</h1>
      <%= simple_form_for @list do |f| %>
      <%= f.input :name %>
      <%= f.input :photo, as: :file%>
      <%= f.submit class: 'btn btn-success text-white' %>
      <% end %>
    </div>
  </div>
</div>
